<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blackout.mydrunkendiaries"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Totoactivity"
            android:label="@string/title_activity_totoactivity" >
        </activity>
        <activity
            android:name=".DetailPartyActivity"
            android:label="@string/title_activity_detail_party" >
        </activity>
        <activity
            android:name=".PartyDetailActivity"
            android:label="@string/title_activity_party_detail" >
        </activity>
    </application>

</manifest>
